  "@babel/cli": "^7.4.4",
  "@babel/core": "^7.4.4",
  "@babel/node": "^7.2.2",
  "@babel/preset-env": "^7.4.4",
  "async": "^2.6.2",
  "nodemon": "^1.19.0"
 },
 "repository": {
  "type": "git",
  "url": "git+https://github.com/oscrouter/oscrouter-cli.git"
 },
